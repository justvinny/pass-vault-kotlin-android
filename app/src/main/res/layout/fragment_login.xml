<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.LoginFragment">

    <ImageView
        android:id="@+id/app_login_logo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="32dp"
        android:cropToPadding="false"
        android:importantForAccessibility="no"
        android:src="@drawable/undraw_secure_login_pdn4"
        app:layout_constraintBottom_toTopOf="@id/circle_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/circle_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_circle_outlined"
        app:layout_constraintBottom_toTopOf="@id/button_1"
        app:layout_constraintEnd_toStartOf="@+id/circle_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:tint="?attr/colorPrimary"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        android:importantForAccessibility="no" />

    <ImageView
        android:id="@+id/circle_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_circle_outlined"
        app:layout_constraintBottom_toTopOf="@id/button_1"
        app:layout_constraintEnd_toStartOf="@+id/circle_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/circle_1"
        app:tint="?attr/colorPrimary"
        android:layout_marginBottom="20dp"
        android:importantForAccessibility="no" />

    <ImageView
        android:id="@+id/circle_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_circle_outlined"
        app:layout_constraintBottom_toTopOf="@id/button_1"
        app:layout_constraintEnd_toStartOf="@+id/circle_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/circle_2"
        app:tint="?attr/colorPrimary"
        android:layout_marginBottom="20dp"
        android:importantForAccessibility="no" />

    <ImageView
        android:id="@+id/circle_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_circle_outlined"
        app:layout_constraintBottom_toTopOf="@id/button_1"
        app:layout_constraintEnd_toStartOf="@+id/circle_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/circle_3"
        app:tint="?attr/colorPrimary"
        android:layout_marginBottom="20dp"
        android:importantForAccessibility="no" />

    <ImageView
        android:id="@+id/circle_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_circle_outlined"
        app:layout_constraintBottom_toTopOf="@id/button_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/circle_4"
        app:tint="?attr/colorPrimary"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="32dp"
        android:importantForAccessibility="no" />

    <Button
        android:id="@+id/button_1"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_1"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_4"
        app:layout_constraintEnd_toStartOf="@+id/button_0"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_2"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_2"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_5"
        app:layout_constraintEnd_toStartOf="@+id/button_clear"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toEndOf="@+id/button_forgot_password"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_3"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_3"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/button_0"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_4"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_7"
        app:layout_constraintEnd_toStartOf="@+id/button_0"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_5"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_5"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_8"
        app:layout_constraintEnd_toStartOf="@+id/button_clear"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toEndOf="@+id/button_forgot_password"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_6"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_6"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/button_0"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_8"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_8"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_0"
        app:layout_constraintEnd_toStartOf="@+id/button_clear"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toEndOf="@+id/button_forgot_password"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_9"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_9"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_clear"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/button_0"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_7"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:text="@string/button_7"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/button_forgot_password"
        app:layout_constraintEnd_toStartOf="@+id/button_0"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_clear"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:layout_marginBottom="24dp"
        android:text="@string/button_clear"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_0"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_0"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:layout_marginBottom="24dp"
        android:text="@string/button_0"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_clear"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/button_forgot_password"
        app:strokeColor="?attr/colorPrimary" />

    <Button
        android:id="@+id/button_forgot_password"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton.Circular"
        android:layout_marginBottom="24dp"
        android:letterSpacing="0"
        android:text="@string/button_forgot_password"
        android:textAllCaps="false"
        android:textSize="13sp"
        app:icon="@drawable/ic_password"
        app:iconGravity="start"
        app:iconPadding="2dp"
        app:iconSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_0"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:strokeColor="?attr/colorPrimary" />

</androidx.constraintlayout.widget.ConstraintLayout>